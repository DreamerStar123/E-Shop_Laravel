{
    "name": "laravel/ui",
    "description": "Laravel UI utilities and presets.",
    "keywords": ["laravel", "ui"],
    "license": "MIT",
    "authors": [
        {
            "name": "Taylor Otwell",
            "email": "taylor@laravel.com"
        }
    ],
    "require": {
        "php": "^7.1.3|^8.0",
        "illuminate/console": "~5.8|^6.0",
        "illuminate/filesystem": "~5.8|^6.0",
        "illuminate/support": "~5.8|^6.0"
    },
    "autoload": {
        "psr-4": {
            "Laravel\\Ui\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Laravel\\Ui\\UiServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
